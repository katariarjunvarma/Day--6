#include <iostream>
#include <vector>

int main() {
    int size;
    std::cout << "Enter the size of the array: ";
    std::cin >> size;
    
    std::vector<int> arr(size);
    std::cout << "Enter the array elements: ";
    for (int i = 0; i < size; ++i) {
        std::cin >> arr[i];
    }
    
    int sum = 0;
    for (int num : arr) {
        sum += num;
    }
    
    std::cout << "Sum of the array: " << sum << std::endl;
    
    return 0;
}
